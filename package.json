{
  "name": "parcel-react-starter",
  "version": "0.1.0",
  "description": "a template for react spa builded by parcel",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel public/index.html --open",
    "build": "cross-env NODE_ENV=production parcel build public/index.html -d build --no-cache --public-url /app/",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "jest --coverage",
    "doc": "esdoc && open ./docs/index.html",
    "lint": "eslint --ext .js src mock test && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock test && npm run lint:style",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "precommit": "npm run lint-staged"
  },
  "repository": "https://stainless.dreamarts.co.jp/INSUITE/INSUITE-GENERAL/tree/master/src/huckleberry",
  "author": "Jifa Jiang <jf_jiang@dreamarts.co.jp>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "less": "^2.7.3",
    "lint-staged": "^6.1.0",
    "parcel-bundler": "1.3.1",
    "postcss-modules": "^1.1.0",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "sinon": "^5.0.7",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "dependencies": {
    "antd": "^3.4.1",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "classname": "^0.0.0",
    "iconfont-plus": "^0.0.1",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.2.2",
    "sweetalert": "^2.1.0"
  },
  "peerDependencies": {
    "immutable": "^3.7.4",
    "prop-types": "15.5.4"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setupTests.js",
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "jsx"
    ],
    "globals": {
      "NODE_ENV": "test"
    },
    "verbose": true,
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
